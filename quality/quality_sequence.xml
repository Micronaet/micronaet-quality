        </record>

        <record model="ir.sequence.type" id="seq_action_type">
                <field name="name">Quality action type</field>
                <field name="code">quality.action</field>
        </record>
        <record model="ir.sequence" id="seq_action_number">
                <field name="name">Quality</field>
                <field name="code">quality.action</field>
                <field name="padding" eval="5" />
                <field name="prefix">ACP</field>
        </record>

        <record model="ir.sequence.type" id="seq_acceptation_type">
                <field name="name">Quality acceptation type</field>
                <field name="code">quality.acceptation</field>
        </record>
        <record model="ir.sequence" id="seq_acceptation_number">
                <field name="name">Quality</field>
                <field name="code">quality.acceptation</field>
                <field name="padding" eval="5" />
